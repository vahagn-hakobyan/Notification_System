
@{
    ViewData["Title"] = "Loginpage";
    Layout = "~/Views/Shared/MainLayout.cshtml";

}

<h1>Login page</h1>
<style>
    #imp1 {
        height: 30px;
        width: 200px;
    }

    #imp2 {
        height: 30px;
        width: 200px;
    }
</style>


<h1>Login</h1>
<form method="post" action="/SignupSignin/Login">
   
    <div class="form-group">
        <label>Email</label>
        <input class="form-control" id="imp1" name="Email" />
    </div>
    <div class="form-group">
        <label>Password</label>
        <input class="form-control" id="imp2" name="Password"  type="password" />
    </div>
    <div>
        <button class="btn btn-warning btn-lg">login</button>
    </div>


</form>

